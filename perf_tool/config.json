{
  "scheduler": {
    "prepare_duration_sec": 3,
    "ramp_up_duration_sec": 10,
    "sustain_duration_sec": 50,
    "ramp_down_duration_sec": 10,
    "close_duration_sec": 1
  },
  "objective": {
    "type": "HTTP_REQUESTS",
    "value": 1000,
    "requests_per_connection": 1
  },
  "http_config": {
    "use_https":1,
    "cert_path":"./server.crt",
    "key_path":"./server.key",
    "client_request_path": "/hello",
    "response_size_hello": 100, 
    "response_size_another": 5242880,
    "response_size_default": 1024
  },
  "interface": {
    "mtu": 2000
  },
  "network": {
    "l2": {
      "mac_address": "00:00:00:00:00:00"
    },
    "l3": {
      "src_ip_start": "192.168.1.10",
      "src_ip_end": "192.168.1.10",
      "dst_ip_start": "192.168.1.1",
      "dst_ip_end": "192.168.1.1"
    },
    "l4": {
      "protocol": "TCP",
      "src_port_start": 1000,
      "src_port_end": 65000,
      "dst_port_start": 8000,
      "dst_port_end": 8000 
    }
  },
  "dpdk": {
    "iface": "ens1",
    "args": "-l1 --vdev eth_af_packet0,iface=ens1,blocksz=4096,framesz=2048,framecnt=512,qpairs=1 --proc-type=primary --file-prefix=rte --no-huge"
  },
  "client_payload": {
    "data": "Hello from client!",
    "size": 18
  },
  "server_response": {
    "data": "Hello from server!",
    "size": 18
  }
}
